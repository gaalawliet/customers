{
  "_version": "1.37.0",
  "sap.app": {
    "id": "zcustomers",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.9.2",
      "toolsId": "a333b279-ca46-419f-82d8-9e8d9b0623b5"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZRDWP_GPIMENTEL_MONITOR_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.96.6",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "zcustomers.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
          "viewType": "XML",
          "async": true,
          "viewPath": "zcustomers.view",
          "controlAggregation": "pages",
          "controlId": "idAppControl",
          "clearControlAggregation": false
        },
        "routes": [
          {
            "name": "RouteView1",
            "pattern": "",
            "target": [
              "TargetView1"
            ]
          },
          {
            "name": "RouteView2",
            "pattern": "View2",
            "target": [
              "TargetView1",
              "TargetView2"
            ]
          }
        ],
        "targets": {
          "TargetView1": {
            "viewType": "XML",
            "viewId": "View1",
            "viewName": "View1",
            "transition": "slide",
            "controlAggregation": "masterPages",
            "viewlevel": 0
          },
          "TargetView2": {
            "viewType": "XML",
            "viewId": "View2",
            "viewName": "View2",
            "transition": "slide",
            "controlAggregation": "detailPages",
            "viewlevel": 1
          }
        }
      },
      "rootView": {
        "viewName": "zcustomers.view.App",
        "type": "XML",
        "async": true,
        "id": "App"
      }
    }
  }